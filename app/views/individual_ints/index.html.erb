<h1>Listing individual_ints</h1>

<table class="table">
  <thead>
    <tr>
      <th>Location</th>
      <th>User</th>
      <th>Dt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @individual_ints.each do |individual_int| %>
      <tr>
        <td><%= individual_int.location %></td>
        <td><%= individual_int.user_id %></td>
        <td><%= individual_int.dt %></td>
        <td><%= link_to 'Show', individual_int %></td>
        <td><%= link_to 'Edit', edit_individual_int_path(individual_int) %></td>
        <td><%= link_to 'Destroy', individual_int, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Individual int', new_individual_int_path %>
