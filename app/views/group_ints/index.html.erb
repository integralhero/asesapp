<h1>Listing group_ints</h1>

<table class="table"> 
  <thead>
    <tr>
      <th>Location</th>
      <th>User</th>
      <th>Dt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_ints.each do |group_int| %>
      <tr>
        <td><%= group_int.location %></td>
        <td><%= group_int.user_id %></td>
        <td><%= group_int.dt %></td>
        <td><%= link_to 'Show', group_int %></td>
        <td><%= link_to 'Edit', edit_group_int_path(group_int) %></td>
        <td><%= link_to 'Destroy', group_int, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group int', new_group_int_path %>
